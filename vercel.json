{
  "name": "dashboard",
  "version": 2,
  "cleanUrls": true,
  "framework": "angular",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    { "src": "/client/source/(.*)", "dest": "/client/dist/$1" }
  ]
}